{
    "name": "nimbly\/minion",
    "version": "2.2.1",
    "description": "Framework and language agnostic remote deployment automation tool",
    "keywords": [
        "deployment",
        "automation",
        "php",
        "remote"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@nimbly.io"
        }
    ],
    "require": {
        "php": ">=7.0",
        "symfony\/console": "~4.0",
        "symfony\/yaml": "^4.0",
        "phpseclib\/phpseclib": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "minion\\": [
                "src\/",
                ".\/"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/nimbly\/minion"
        }
    ],
    "bin": [
        "bin\/minion"
    ]
}