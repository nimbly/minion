{
    "name": "nimbly\/minion",
    "version": "2.0",
    "description": "Framework and language agnostic remote deployment automation tool",
    "keywords": [
        "deployment",
        "automation",
        "php",
        "remote"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@nimbly.io"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/console": "^3.2",
        "symfony/yaml": "^3.2",
        "phpseclib/phpseclib": "~2.0"
    },

    "autoload": {
        "psr-4": {
            "minion\\": ["src/", "./"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/nimbly\/minion"
        }
    ],
    "bin": [
        "minion"
    ]
}